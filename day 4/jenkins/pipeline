pipeline{
    agent any
    stages{
        stage("Print Hello"){
            steps{
                sh 'pwd'
                git branch:'main',url:'https://github.com/jagafaster/jenkins-nginx.git'
                echo 'Hello'
                sh 'ls'
            }
            stage('Docker'){
                steps{
                    sh'docker build-t web134.'
                    sh'docker run -d -p 80:80 web134'
                }
            }
            
        }
    }
}